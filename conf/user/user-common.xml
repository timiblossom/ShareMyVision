<?xml version="1.0" encoding="UTF-8"?>
<beans
  xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">
  
  
  <bean id="sessionFactoryResource" class="org.springframework.beans.factory.config.MethodInvokingFactoryBean">
        <property name="staticMethod" value="com.smv.service.user.config.ResourceManager.setSessionFactory"/>
        <property name="arguments"><list><ref bean="sessionFactory"/></list></property>
  </bean>
  
</beans>

 